      </condition>
      <volume>
        <factor>1</factor>
      </volume>
    </ap-off>

		<!-- trimwheel sound -->

		<trimming>
			<name>trimming</name>
			<mode>once</mode>
			<path>Aircraft/737-400/Sounds/trim.wav</path>
			<condition>
				<property>/b737/sound/stab-trim</property>
			</condition>
			<volume>
				<factor>1</factor>
			</volume>
		</trimming>
		
   <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/737-400/Sounds/stall.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>/fdm/jsbsim/aero/alpha-deg</property>
            <value>12.0</value>
          </greater-than>
          <greater-than>
            <property>/velocities/airspeed-kt</property>
            <value>20</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <factor>1</factor>
      </volume>
    </stall>
		
		<!-- Ground Services-->

		<fuelpump>
			<name>fuel truck pump transfer</name>
			<path>/Aircraft/737-400/Sounds/electricalengine.wav</path>
			<mode>looped</mode>
			<condition>
				<less-than>	
					<property>/velocities/groundspeed-kt</property>
					<value>1</value>
				</less-than>
			</condition>
			<volume>
				<property>/services/fuel-truck/transfer</property>
				<value>1</value>
			</volume>
        <!--volume>
            <property>sim/sound/Ovolume</property>
        </volume-->
        <position>
        	<x-m>0.25</x-m>
        	<y-m>-1.0</y-m>
        	<z-m>1.0</z-m>
        </position>
        <reference-dist>60.0</reference-dist>
        <max-dist>50.0</max-dist>
    </fuelpump>

