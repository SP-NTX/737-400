				<property>/position/altitude-agl-ft</property>
				<value>2520</value>
			</less-than>
			<greater-than>
				<property>/position/altitude-agl-ft</property>
				<value>2480</value>
			</greater-than>	
			<less-than>
				<property>/velocities/vertical-speed-fps</property>
				<value>0</value>
			</less-than>
		</condition>
    	</alt2500>

    <gpws>
    	<name>appr_mins</name>
    	<mode>once</mode>
    	<type>avionics</type>		
    	<path>Aircraft/737-400/Sounds/approaching-minimums.wav</path>
    	<condition>
    		<less-than>
    			<property>/position/altitude-agl-ft</property>
    			<value>380</value>
    		</less-than>
    		<greater-than>
    			<property>/position/altitude-agl-ft</property>
    			<value>300</value>
    		</greater-than>			
    		<less-than>
    			<property>/velocities/vertical-speed-fps</property>
    			<value>0</value>
    		</less-than>
    	</condition>
    </gpws>

    <leftgear>
      <name>Left Main</name>
      <path>Aircraft/737-400/Sounds/tires.wav</path>
      <mode>once</mode>
      <condition>
        <and>
          <property>gear/gear[1]/wow</property>
          <greater-than>
            <property>/velocities/airspeed-kt</property>
            <value>30</value>
          </greater-than>
          <greater-than>
            <property>/velocities/speed-down-fps</property>
            <value>0.3</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <offset>-0.75</offset>
      </volume>
<!--      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
      </volume> -->
    </leftgear>
    <rightgear>
      <name>Right Main</name>
      <path>Aircraft/737-400/Sounds/tires.wav</path>
      <mode>once</mode>
      <condition>
        <and>
          <property>gear/gear[2]/wow</property>
          <greater-than>
            <property>/velocities/airspeed-kt</property>
            <value>30</value>
          </greater-than>
          <greater-than>
            <property>/velocities/speed-down-fps</property>
            <value>0.3</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <offset>-0.75</offset>
      </volume>
<!--      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
      </volume> -->
    </rightgear>
    <nosegear>
      <name>Nose Main</name>
      <path>Aircraft/737-400/Sounds/tires.wav</path>
      <mode>once</mode>
      <condition>
        <and>
          <property>gear/gear[0]/wow</property>
          <greater-than>
            <property>/velocities/airspeed-kt</property>
            <value>30</value>
          </greater-than>
          <greater-than>
            <property>/velocities/speed-down-fps</property>
            <value>0.3</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <offset>-0.85</offset>
      </volume>
    </nosegear>

    <rain>
    	<name>rain</name>
    	<mode>looped</mode>
    	<path>Aircraft/737-400/Sounds/rain.wav</path>
    	<condition>
    		<greater-than>
    			<property>/environment/rain-norm</property>
    			<value>0.2</value>
    		</greater-than>
    	</condition>
    	<volume>
    		<property>/environment/rain-norm</property>
    		<factor>1.00</factor>
    		<offset>0.00</offset>
    		<min>0.4</min>
    		<max>0.9</max>
    	</volume>
    	<pitch>
    		<property>/velocities/airspeed-kt</property>
    		<factor>0.001</factor>
    		<offset>1.0</offset>
    	</pitch>
    </rain>
    
     <seatbelt-on>
      <name>seatbelt-sign-on</name>
      <mode>once</mode>
      <path>Aircraft/737-400/Sounds/cabincall.wav</path>
      <condition>
        <and>
          <property>systems/electrical/on</property>
          <property>/controls/switches/seatbelt-sign</property>
        </and>
      </condition>
      <volume>
        <factor>1</factor>
      </volume>
    </seatbelt-on>
    
    <nosmoke-on>
      <name>nosmoking-sign-on</name>
      <mode>once</mode>
      <path>Aircraft/737-400/Sounds/cabincall.wav</path>
      <condition>
        <and>
          <property>systems/electrical/on</property>
          <property>/controls/switches/nosmoking-sign</property>
        </and>
      </condition>
      <volume>
        <factor>1</factor>
      </volume>
    </nosmoke-on>


    <avionics>
    	<name>avionics</name>
    	<path>Aircraft/737-400/Sounds/avionics.wav</path>
    	<mode>looped</mode>
    	<condition>
    		<property>sim/current-view/internal</property>
    		<greater-than>
    			<property>systems/electrical/outputs/efis</property>
    			<value>0</value>
    		</greater-than>
    	</condition>
    	<position>
    		<x>-17</x>
    		<y>0.01</y>
    		<z>-1.95</z>
    	</position>	
    	<volume>
    		<factor>0.2</factor>
    		<max>0.5</max>
    	</volume>
    	<reference-dist>1.2</reference-dist>
    	<max-dist>5</max-dist>
    </avionics>

    <switch>
    	<name>click on</name>
    	<mode>once</mode>
    	<path>Aircraft/737-400/Sounds/click.wav</path>
    	<condition>
    		<property>/controls/switches/switch</property>
    	</condition>
    	<volume>
    		<factor>0.45</factor>
    	</volume>
    </switch>

    <cockpitdoor>
    	<name>open_close</name>
    	<mode>once</mode>
    	<path>Aircraft/737-400/Sounds/door_close.wav</path>
    	<condition>
    		<property>/controls/doors/cockpitdoor/sound</property>
    	</condition>
    	<volume>
    		<factor>0.8</factor>
    	</volume>
    	<max-dist>20.0</max-dist>
    </cockpitdoor>
        
        <!-- Crashed -->

        <crash>
        	<name>intense ground contact</name>
        	<mode>once</mode>
        	<path>Aircraft/737-400/Sounds/crash.wav</path>
        	<property>sim/crashed</property>
        	<volume>
        		<factor>1</factor>
        	</volume>
        	<pitch>
        		<factor>1.5</factor>
        	</pitch>
        </crash>

   <wind>
        <name>wind</name>
        <mode>looped</mode>
        <path>Aircraft/737-400/Sounds/Wind.wav</path>
        <property>/velocities/airspeed-kt</property>
        <volume>
            <property>/position/altitude-ft</property>
            <factor>-0.000015</factor>
            <offset>1.0</offset>
            <min>0.1</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>/velocities/airspeed-kt</property>
            <factor>0.0015</factor>
            <min>0.03</min>
            <max>0.25</max>
        </volume>
        <pitch>
            <property>/velocities/airspeed-kt</property>
            <factor>0.01</factor>
            <offset>1.25</offset>
        </pitch>
        <position>
            <x>-24</x>
            <y>0</y>
            <z>0.86</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </wind>

    </fx>
    
</PropertyList>

