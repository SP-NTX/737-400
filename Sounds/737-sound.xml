     <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Aircraft/737-400/Sounds/rumble.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>/velocities/groundspeed-kt</property>
            <value>9</value>
          </greater-than>
          <or>
            <property>/gear/gear[0]/wow</property>
            <property>/gear/gear[1]/wow</property>
            <property>/gear/gear[2]/wow</property>
          </or>
        </and>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.6</factor>
      </volume>
    </rumble>
		
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/737-400/Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume>
        <factor>0.45</factor>
      </volume>
    </flap>

     <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/737-400/Sounds/gear.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <pitch>
        <internal>dt_play</internal>
        <offset>1.0</offset>
        <factor>-0.001</factor>
      </pitch>
    </gear>
		
		<overspeed>
			<name>Overspeed warning</name>
			<path>Aircraft/737-400/Sounds/overspeed.wav</path>
			<mode>looped</mode>
			<condition>
				<greater-than>
					<property>velocities/airspeed-kt</property>
					<value>345</value>
				</greater-than>
			</condition>
			<volume>
				<factor>1.0</factor>
			</volume>
		</overspeed>
	
