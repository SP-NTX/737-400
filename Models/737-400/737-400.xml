	<type>select</type>
	<object-name>winglets</object-name>
	<condition>
	    <!--property>sim/model/winglets</property-->
	    <equals>
		<property>sim/model/variant</property>
		<value>1</value>
	    </equals>
	</condition>
    </animation>

      <animation>
		<name>Winglet illumination</name>
		<type>material</type>
               <object-name>WingletL</object-name>
                  <object-name>WingletR</object-name>
 		<emission>
			<red>0.25</red>
			<green>0.25</green>
			<blue>0.25</blue>
			<factor-prop>controls/lighting/logo-lights</factor-prop>
		</emission>
	</animation>

